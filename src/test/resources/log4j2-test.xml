<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p %d %50.50c: %m%n"/>
        </Console>
    </Appenders>

    <Loggers>

        <Logger name="org.mule.extension.aws" level="trace" />

        <AsyncRoot level="INFO">
            <AppenderRef ref="Console"/>
        </AsyncRoot>
    </Loggers>

</Configuration>